{
  "name": "rentease-api",
  "version": "1.0.0",
  "description": "API for the RentEase platform",
  "main": "server.js",
  "type": "module",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "test-server": "node start-server.js",
    "test-endpoints": "node test-endpoints.js",
    "sync-quantities": "node scripts/syncQuantities.js",
    "sync-quantities:dry-run": "node scripts/syncQuantities.js --dry-run",
    "setup": "node setup/setup.js",
    "setup:db": "node setup/setup.js",
    "setup:buckets": "node setup/create_buckets.js",
    "setup:schema": "psql -h your-db-host -U postgres -d postgres -f setup/db.sql",
    "setup:data": "psql -h your-db-host -U postgres -d postgres -f setup/insert_data.sql",
    "migrate:google-oauth": "psql -h your-db-host -U postgres -d postgres -f migrations/add_google_oauth_fields.sql"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.49.8",
    "bcrypt": "^6.0.0",
    "bcryptjs": "^3.0.2",
    "compression": "^1.8.0",
    "cors": "^2.8.5",
    "dotenv": "^16.5.0",
    "express": "^4.21.2",
    "google-auth-library": "^10.2.1",
    "helmet": "^8.1.0",
    "joi": "^17.13.3",
    "jsonwebtoken": "^9.0.2",
    "moment": "^2.30.1",
    "morgan": "^1.10.0",
    "multer": "^2.0.0",
    "node-fetch": "^2.7.0",
    "nodemailer": "^7.0.3",
    "slugify": "^1.6.6",
    "socket.io": "^4.8.1",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "nodemon": "^3.1.10"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "author": "NISIO",
  "license": "NISIO"
}
